---
import Containter from "../components/Containter.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import Hero from "../components/heros/Hero.astro";
import Sectionhead from "../components/SectionHead.astro";
import Faq from "../components/misc/Faq.astro";
import Feature1 from "../components/misc/Feature1.astro";
import Feature2 from "../components/misc/Feature2.astro";
import Carousel from "../components/misc/Carousel.astro";

import Header from "../components/headers/Header1.astro";
---

<BaseLayout title="YCWD | Home">
  <Header />
