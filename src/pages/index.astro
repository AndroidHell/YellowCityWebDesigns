      <Fragment slot="title">Home Page</Fragment>
      <Fragment slot="desc">Magical Subtitle</Fragment>
    </Sectionhead>
    <section class="bg-white pt-4 dark:bg-gray-800" id="intro">
      <div
        class="mx-auto max-w-screen-xl gap-16 px-4 py-8 font-light text-gray-500 dark:text-gray-400 sm:text-lg lg:px-6 lg:py-16"
      >
        <h2
          class="mb-4 text-4xl font-extrabold tracking-tight text-gray-900 dark:text-white"
        >
          Welcome weary traveler
