---
import Container from "../components/Container.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import PageHeader from "../components/PageHeader.astro";
import FaqAccordian from "../components/FaqAccordian.astro";
import ServicesMain from "../components/ServicesMain.astro";
import Pricing from "../components/Pricing.astro";
import CtaBanner from "../components/CtaBanner.astro";

const title = "Our Services";
const desc = "";
const image = "/page-header-light.webp";
---

<BaseLayout title="Services" description="Services">
  <PageHeader title={title} desc={desc} image={image} />
  <Container>
    <ServicesMain />
    <Pricing />
    <FaqAccordian />
  </Container>
  <CtaBanner />
</BaseLayout>
