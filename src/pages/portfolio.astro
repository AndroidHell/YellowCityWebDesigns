---
import Container from "../components/Container.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import SectionHead from "../components/SectionHead.astro";
import ClientGallery from "../components/ClientGallery.astro";

const title = "Our Portfolio";
const desc = "Check out what we have made for people";
const image = "/hero-inner.webp";
---

<BaseLayout title="Gallery" description="Gallery">
  <SectionHead title={title} desc={desc} image={image} />
  <Container>
    <ClientGallery />
  </Container>
</BaseLayout>
