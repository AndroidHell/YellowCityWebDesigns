---
import Container from "../components/Container.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import HeroInner from "../components/HeroInner.astro";
import Faq from "../components/Faq.astro";
import AboutHead from "../components/AboutHead.astro";
import Pricing from "../components/Pricing.astro";
import CtaBanner from "../components/CtaBanner.astro";

const title = "We Are YCWD";
const desc = "";
const image = "/hero-inner.webp";
---

<BaseLayout title="About Us" description="About Us">
  <HeroInner title={title} desc={desc} image={image} />
  <Container>
    <AboutHead />
    <Pricing />
    <Faq />
  </Container>
  <CtaBanner />
</BaseLayout>
