---
import Containter from "../components/Containter.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import Sectionhead from "../components/SectionHead.astro";
import Faq from "../components/misc/Faq.astro";
import AboutHead from "../components/misc/AboutHead.astro";
import Pricing from "../components/misc/Pricing.astro";
---

<BaseLayout title="About Us" description="About Us">
  <Containter>
    <Sectionhead>
      <Fragment slot="title">We Are YCWD</Fragment>
      <Fragment slot="desc">
        We make websites for for people who need websites to just work. We make
        websites for people like you.
      </Fragment>
    </Sectionhead>
    <AboutHead />
    <Pricing />
    <Faq />
  </Containter>
</BaseLayout>
