---
import Container from "../components/Container.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import SectionHead from "../components/SectionHead.astro";
import Faq from "../components/Faq.astro";
import AboutHead from "../components/AboutHead.astro";
import Pricing from "../components/Pricing.astro";

const title = "We Are YCWD";
const desc =
  "We make websites for people who need websites to just work. We make websites for people like you";
const image = "/hero-inner.webp";
---

<BaseLayout title="About Us" description="About Us">
  <SectionHead title={title} desc={desc} image={image} />
  <Container>
    <AboutHead />
    <Pricing />
    <Faq />
  </Container>
</BaseLayout>
